{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "app",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file", ".env/secret.env"]
    },
    {
      "name": "app (web, wasm)",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome", "--wasm"]
    },
    {
      "name": "app (profile mode)",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--dart-define-from-file", ".env/secret.env"]
    },
    {
      "name": "app (release mode)",
      "cwd": "app",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": ["--dart-define-from-file", ".env/secret.env"]
    }
  ]
}
